
<%= form_with(model: @user, local: true) do | f | %>
<%= f.label :name, "Name" %>
<%= f.text_field :name %>
<%= f.label :last_name, "Last Name" %>
<%= f.text_field :last_name %>
<%= f.label :username, "Username" %>
<%= f.text_field :username %>
<%= f.label :email, "email" %>
<%= f.email_field :email %>
<%= f.label :password, "password" %>
<%= f.password_field :password %>
<%= f.submit "Sign up" %>


<% end %>